{
  "name": "nobu-stack-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node --require ./mocks ./build/server.js\"",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "format:repo": "yarn format && yarn lint:repo -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "yarn lint -- --config ./.eslintrc.repo.js",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "cross-env PORT=3000 start-server-and-test start:mocks http://127.0.0.1:8811 \"npx cypress run --browser chromium --headless\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@conform-to/react": "^0.9.0",
    "@conform-to/zod": "^0.9.0",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@prisma/client": "^5.5.2",
    "@react-email/components": "^0.0.10",
    "@remix-run/css-bundle": "*",
    "@remix-run/express": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/server-runtime": "*",
    "argon2": "^0.31.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "daisyui": "^3.9.4",
    "express": "^4.18.2",
    "isbot": "^3.7.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "otplib": "^12.0.1",
    "prom-client": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^1.9.5",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "remixicon-react": "^1.0.0",
    "source-map-support": "^0.5.21",
    "thirty-two": "^1.0.2",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@playwright/test": "^1.40.1",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/compression": "^1.7.4",
    "@types/eslint": "^8.44.6",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.7",
    "@types/node": "^18.18.6",
    "@types/nodemailer": "^6.4.13",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/source-map-support": "^0.5.9",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "cookie": "^0.5.0",
    "cypress": "^13.4.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prefer-let": "^3.0.1",
    "execa": "^5.1.1",
    "happy-dom": "^12.10.3",
    "msw": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.2",
    "remix-development-tools": "*",
    "remix-flat-routes": "^0.6.2",
    "start-server-and-test": "^2.0.1",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --skipProject --require tsconfig-paths/register prisma/seed.ts"
  }
}
